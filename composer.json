{
    "name": "lukehopkins/email-list",
    "description": "A plugin for saving emails to a list",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "email list"
    ],
    "support": {
        "docs": "https://github.com/luke-hopkins/email-list/blob/master/README.md",
        "issues": "https://github.com/luke-hopkins/email-list/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Luke Hopkins",
            "homepage": "https://github.com/luke-hopkins"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
            "lukehopkins\\emaillist\\": "src/"
        }
    },
    "extra": {
        "name": "Email List",
        "handle": "email-list",
        "developer": "Luke Hopkins",
        "developerUrl": "https://github.com/luke-hopkins",
        "documentationUrl": "https://github.com/luke-hopkins/email-list/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/luke-hopkins/email-list/master/CHANGELOG.md",
        "components": {
            "email": "lukehopkins\\emaillist\\services\\Email"
        },
        "class": "lukehopkins\\emaillist\\EmailList"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "craftcms/rector": "dev-main"
    }
}
